@model DM.FiveDay
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script language="javascript">
    function mySelect() {
        alert(document.getElementById("yourDropName").select());
    }
</script>
<h1>View</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" style="border: solid; border-width: 1px; border-color: green;">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Symbol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>@Html.DropDownList("yourDropName", Model.Tickers, new { @class = "form-control", @onselect = "mySelect();" } )</th>
    </tr>
    <tr>
        <th></th>
    </tr>
    </thead>
    <tbody>
        @using (Html.BeginForm("Index", "FiveDay", FormMethod.Post))
        {
            <tr><td><input type="submit" value="submit" /></td></tr>
        }
            

        </tbody>
</table>